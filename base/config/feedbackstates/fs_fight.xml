			<!-- This event type is allowed-->
			<Event>
				<EventType>FEEDBACK_EVENT_ENTITY_HURT</EventType>
				<!-- negative value means, all of this type-->
				<Parameter1>-1</Parameter1>
				<Parameter2>-1</Parameter2>
				<Parameter3>-1</Parameter3>
			</Event>
			<Data>
				<!--Time this event is hold in to be played memory-->
				<LifeTime>1</LifeTime>
				<!--Priority of sound, lower priority is better-->
				<Priority>100</Priority>
				<!--Ignore Events for time-->
				<Ignore>
					<Event>
						<!--Empty type means ignore own event-->
						<EventType>FEEDBACK_EVENT_ENTITY_HURT</EventType>
						<!-- -1 means, all of this type-->
						<!-- <= -2 means same as input parameter..warning use only with events that has same parameter value-->
						<Parameter1>-1</Parameter1>
						<Parameter2>-1</Parameter2>
						<Parameter3>-1</Parameter3>
					</Event>
					<!--Ignore this event for given time from now-->
					<Time>10</Time>
				</Ignore>
			</Data>
